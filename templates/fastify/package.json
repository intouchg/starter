{
	"description": "NodeJS Fastify server",
	"scripts": {
		"start": "node server/index.js",
		"dev": "NODE_ENV=development nodemon -w server/index.js",
		"build": "webpack --mode production --progress",
		"dev-build": "webpack --mode development --progress",
		"prepare": "is-ci || husky install",
		"release": "npm run build",
		"lint": "eslint --fix \"client/**/*.{js,jsx,ts,tsx}\"",
		"prettier": "prettier --write .",
		"pretty-quick": "pretty-quick --staged"
	},
	"dependencies": {
		"@babel/runtime": "7.14.6",
		"dotenv": "10.0.0",
		"fastify": "3.18.1",
		"fastify-http-proxy": "6.0.0",
		"fastify-static": "4.2.2"
	},
	"devDependencies": {
		"@babel/core": "7.14.6",
		"@babel/eslint-parser": "7.14.7",
		"@babel/eslint-plugin": "7.14.5",
		"@babel/plugin-transform-runtime": "7.14.5",
		"@babel/preset-env": "7.14.7",
		"@babel/preset-react": "7.14.5",
		"@babel/preset-typescript": "7.14.5",
		"@intouchg/eslint-config": "latest",
		"@intouchg/fastify-webpack-hot": "latest",
		"@intouchg/prettier-config": "latest",
		"@typescript-eslint/eslint-plugin": "4.28.0",
		"@typescript-eslint/parser": "4.28.0",
		"babel-loader": "8.2.2",
		"babel-plugin-styled-components": "1.12.0",
		"brotli-webpack-plugin": "1.1.0",
		"compression-webpack-plugin": "8.0.1",
		"copy-webpack-plugin": "9.0.1",
		"eslint": "7.28.0",
		"eslint-config-prettier": "8.3.0",
		"eslint-plugin-react": "7.24.0",
		"eslint-plugin-react-hooks": "4.2.0",
		"html-loader": "2.1.2",
		"html-webpack-plugin": "5.3.1",
		"husky": "6.0.0",
		"is-ci": "3.0.0",
		"nodemon": "2.0.10",
		"prettier": "2.3.1",
		"pretty-quick": "3.1.1",
		"typescript": "4.3.4",
		"webpack": "5.40.0",
		"webpack-cli": "4.7.2",
		"webpack-dev-server": "3.11.2"
	}
}
